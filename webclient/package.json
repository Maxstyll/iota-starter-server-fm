{
	"name": "iota-starter-server-fleetmanagement-webclient",
	"version": "0.0.1",
	"private": true,
	"engines": {
		"npm": "2.x"
	},
	"scripts": {
		"start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\"",
		"lite": "lite-server -c ./bs-config.json",
		"postinstall": "npm ls typings && typings install || echo .",
		"tsc": "tsc",
		"tsc:w": "tsc -w",
		"typings": "typings",
		"bundle": "node bin/bundler.js",
		"bundle:prod": "node bin/bundler.js --prod"
	},
	"dependencies": {
	    "@angular/common":  "2.0.0-rc.1",
	    "@angular/compiler":  "2.0.0-rc.1",
	    "@angular/core":  "2.0.0-rc.1",
	    "@angular/http":  "2.0.0-rc.1",
	    "@angular/platform-browser":  "2.0.0-rc.1",
	    "@angular/platform-browser-dynamic":  "2.0.0-rc.1",
	    "@angular/router":  "2.0.0-rc.1",
	    "@angular/router-deprecated":  "2.0.0-rc.1",
	    "@angular/upgrade":  "2.0.0-rc.1",
	    
		"systemjs": "0.19.27",
		"core-js": "^2.4.0",
		"reflect-metadata": "^0.1.3",
		"rxjs": "5.0.0-beta.6",
		"zone.js": "^0.6.12",

		"angular2-in-memory-web-api": "0.0.8",
		"bootstrap": "^3.3.6"
	},
	"devDependencies": {
		"concurrently": "^2.0.0",
		"lite-server": "^2.2.0",
		"typescript": "^1.8.10",
		"typings":"^1.3.2",
		"systemjs-builder": "^0.15.17",
		"yargs": "^4.7.1"
	}
}
